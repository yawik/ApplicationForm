{
  "name": "yawik-application-form",
  "description": "application form with file upload and preview based on quasar framework",
  "productName": "Yawik Application Form",
  "author": "Ivaylo Gelov",
  "private": true,
  "scripts": {
    "dev": "quasar dev",
    "build": "quasar build",
    "lint": "eslint --ext .js,.vue src",
    "lintfix": "eslint --ext .js,.vue src --fix",
    "lint-css": "stylelint src/**/*.{vue,css,scss}",
    "lintfix-css": "stylelint src/**/*.{vue,css,scss} --fix",
    "prepare": "custompatch",
    "test": "echo \"See package.json => scripts for available tests.\" && exit 0",
    "semantic-release": "semantic-release",
    "test:ci": "npx cypress run",
    "test:unit": "jest --updateSnapshot",
    "test:unit:ci": "jest --ci",
    "test:unit:coverage": "jest --coverage",
    "test:unit:watch": "jest --watch",
    "test:unit:watchAll": "jest --watchAll",
    "serve:test:coverage": "quasar serve test/jest/coverage/lcov-report/ --port 8788",
    "concurrently:dev:jest": "concurrently \"quasar dev\" \"jest --watch\"",
    "test:e2e": "cross-env E2E_TEST=true start-test \"quasar dev\" http-get://localhost:8080 \"cypress open\"",
    "test:e2e:ci": "cross-env E2E_TEST=true start-test \"quasar dev\" http-get://localhost:8080 \"cypress run\""
  },
  "dependencies": {
    "@quasar/extras": "^1.15.3",
    "@quasar/quasar-ui-qoverlay": "^2.0.1",
    "axios": "^0.21.1",
    "core-js": "^3.6.5",
    "hellojs": "^1.19.3",
    "mitt": "^3.0.0",
    "quasar": "^2.8.4",
    "vue": "^3.0.0",
    "vue-i18n": "^9.1.6",
    "vue-router": "^4.0.0",
    "vuex": "^4.0.2",
    "vuex-persistedstate": "^4.0.0-beta.3"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-0",
    "@babel/eslint-parser": "^7.13.14",
    "@kazupon/vue-i18n-loader": "^0.3.0",
    "@quasar/app-webpack": "^3.6.1",
    "@quasar/quasar-app-extension-testing": "^1.0.5",
    "@quasar/quasar-app-extension-testing-e2e-cypress": "^4.2.1",
    "@quasar/quasar-app-extension-testing-unit-jest": "3.0.0-alpha.10",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/gitlab": "^6.0.5",
    "@semantic-release/npm": "^7.0.8",
    "babel-loader": "^8.2.4",
    "custompatch": "^1.0.17",
    "cypress": "^9.2.0",
    "dotenv-flow": "^3.2.0",
    "eslint": "^7.23.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-cypress": "^2.11.1",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-quasar": "^1.1.0",
    "eslint-plugin-vue": "^7.8.0",
    "eslint-webpack-plugin": "^3.1.1",
    "semantic-release": "^17.3.0",
    "stylelint": "^13.9.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-scss": "^3.18.0",
    "stylelint-webpack-plugin": "^1.2.3"
  },
  "release": {
    "branches": [
      "master",
      "next"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/gitlab",
        {
          "gitlabUrl": "https://gitlab.com/cross-solution1/applicationform.git",
          "assets": [
            {
              "path": "index.js",
              "label": "Module"
            },
            {
              "path": "README.md",
              "label": "Documentation"
            },
            {
              "path": "LICENSE",
              "label": "License"
            }
          ]
        }
      ]
    ]
  },
  "browserslist": [
    "last 2 years, >3%, not dead"
  ],
  "engines": {
    "node": ">= 10.18.1",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
